
<%= simple_form_for(@stock) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :quote %>
    <%= f.input :companyName %>
    <%= f.input :primaryExchange %>
    <%= f.input :sector %>
    <%= f.input :calculationPrice %>
    <%= f.input :open %>
    <%= f.input :close %>
    <%= f.input :high %>
    <%= f.input :low %>
    <%= f.input :latestPrice %>
    <%= f.input :latestSource %>
    <%= f.input :latestTime %>
    <%= f.input :latestVolume %>
    <%= f.input :iexRealTimePrice %>
    <%= f.input :previousClose %>
    <%= f.input :changePercent %>
    <%= f.input :iexVolume %>
    <%= f.input :avgTotalVolume %>
    <%= f.input :iexBidPrice %>
    <%= f.input :iexBidsize %>
    <%= f.input :iexAskPrice %>
    <%= f.input :iexAskSize %>
    <%= f.input :marketCap %>
    <%= f.input :peRatio %>
    <%= f.input :week52High %>
    <%= f.input :week52Low %>
    <%= f.input :ytdChange %>
    <%= f.association :position %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
