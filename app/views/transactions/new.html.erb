<div class="card">
  <div class="card-header">New Trade</div>
  <div class="card-body">
    <form action="/" id="new-trade">
      <p><span id="search-message"></span></p>
      Ticker<br />
      <input type="text" name="ticker" id="ticker" class="form-control" value="<%=@searchTicker %>">
      <br />
      <input type="submit" value="Search" class="btn btn-dark"> <br />
      <br />
    </form>
    <%= render 'form', transaction: @transaction %>
  </div>
</div>
</div>
</div>
</div>
